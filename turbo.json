{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    },
    "docs:extract": {
      "dependsOn": ["^docs:extract"],
      "outputs": []
    },
    "docs:generate": {
      "dependsOn": ["^docs:generate"],
      "outputs": ["temp/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
